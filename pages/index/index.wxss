view {
  /* color: var(--themeColor); */
}

.index_swiper swiper {
  height: 340rpx;
}

.index_swiper image {
  width: 100%;
}

/* 导航 */
.index_nav {
  display: flex;
}

.index_nav navigator {
  flex-grow: 1;
  padding: 20rpx;
}

.index_nav image {
  width: 100%;
}



/* 楼层 */
.floor_item {
  padding-top: 15rpx;
}

.floor_title image {
  width: 100%;
}


.floor_image {
  overflow: hidden;
}

.floor_image navigator {
  float: left;
  width: 30%;
}

.floor_image image {
  width: 100%;
  height: 100%;
}

.floor_image navigator:nth-child(2) {
  padding-bottom: 15rpx;
}

.floor_image navigator:nth-child(3) {
  padding-bottom: 15rpx;
}

.floor_image navigator:nth-child(n+2) {
  height: 180rpx !important;
  padding-left: 15rpx;
}